{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "ui",
      "image": "452359159316.dkr.ecr.sa-east-1.amazonaws.com/app-ui",
      "hostname": "ui",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 4200,
          "containerPort": 4200
        }
      ]
    },
    {
      "name": "nginx",
      "image": "452359159316.dkr.ecr.sa-east-1.amazonaws.com/app-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["api"],
      "memory": 512
    }
  ]
}
